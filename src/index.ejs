    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <div data-id="hi">
        <%= htmlWebpackPlugin.options.data.hi %>
    </div>
    <div>
        <label for="language-select">选择语言：</label>
        <select id="language-select" name="language">
